# URL base do app no browser (usada para montar links canônicos/redirects).
NEXT_PUBLIC_APP_URL=
# URL pública da API para chamadas no client (fallback quando API_URL não está definida).
NEXT_PUBLIC_API_URL=
# URL da API para uso no servidor (ex.: auth e SSR).
API_URL=
# Chave do ImgBB para upload de imagens.
NEXT_PUBLIC_IMGBB_KEY=
# Chave da API do Google Maps para mapas e autocomplete.
NEXT_PUBLIC_GOOGLE_MAPS_API_KEY=
# Ambiente de execução: development | test | production.
NODE_ENV=
